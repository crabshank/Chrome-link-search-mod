{
  "name": "Chrome quick history search",
  "short_name": "Link search",
  "description": "Easily manage your browser history.",
  "manifest_version": 3,
  "version": "2024.1",
  "offline_enabled": true,
  "action": {
    "default_icon": "assets/icons/searchHistory128.png",
    "default_popup": "index.html",
    "default_title": "Chrome link search"
  },

  "permissions": [
    "history",
	"processes",
	"tabs",
	"activeTab",
	"clipboardWrite"
  ],
  "icons": {
    "128": "assets/icons/searchHistory128.png"
  }
}